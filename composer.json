{
  "name": "faridlab/salt-laravel",
  "description": "",
  "keywords": ["Laravel"],
  "authors": [
    {
      "name": "Farid Hidayat",
      "email": "farid@startapp.id"
    }
  ],
  "homepage": "https://github.com/faridlab/salt-laravel-core",
  "require": {
    "php": "^8.0.2",
    "guzzlehttp/guzzle": "^7.2",
    "laravel/framework": "^9.11",
    "laravel/sanctum": "^2.14.1",
    "laravel/tinker": "^2.7"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9.1",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.4",
    "nunomaduro/collision": "^6.1",
    "phpunit/phpunit": "^9.5.10",
    "spatie/laravel-ignition": "^1.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "SaltLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SaltLaravel\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "SaltLaravel\\Providers\\SaltLaravelServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit -c ./phpunit.xml --colors=always",
    "analysis": "vendor/bin/phpstan analyse"
  }
}
